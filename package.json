{
  "name": "muk",
  "version": "0.0.12",
  "main": "Main.ts",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "axios",
          "react-native-uuid",
          "mobx",
          "mobx-react",
          "mobx-persist-store",
          "@stomp/stompjs",
          "text-encoding",
          "i18n-js",
          "react-native-iap"
        ]
      }
    }
  },
  "scripts": {
    "start": "npx expo start",
    "start-dev": "npx expo start --dev-client",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "prebuild": "npx expo prebuild",
    "build": "eas build -p android -e apk",
    "upgrade": "npx expo install --check"
  },
  "dependencies": {
