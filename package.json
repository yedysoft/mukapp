    "@types/i18n-js": "^3.8.9",
    "axios": "1.7.9",
    "expo": "^52.0.23",
    "expo-build-properties": "~0.13.1",
    "expo-constants": "~17.0.2",
    "expo-dev-client": "~5.0.7",
    "expo-device": "~7.0.1",
    "expo-font": "~13.0.1",
    "expo-image": "~2.0.3",
